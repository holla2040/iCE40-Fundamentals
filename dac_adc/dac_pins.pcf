# DAC Baby Step - Pin Constraints
# Target: iCE40 HX1K (adjust for your board)

# Clock - 12 MHz
set_io clk 21

# Trigger input (rising edge starts sweep)
set_io trigger_in 55

# DAC SPI (DAC121S101)
set_io dac_cs_n  38
set_io dac_sclk  37
set_io dac_mosi  36

# Status outputs
set_io busy 99
set_io done 98
