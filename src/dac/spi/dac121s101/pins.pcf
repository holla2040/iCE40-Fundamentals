# DAC Project - Pin Constraints
# Target: Nandland Go Board (iCE40HX1K-VQ100)

# Clock - 25 MHz
set_io clk 15

# Trigger input (rising edge starts sweep)
set_io trigger_in 53    # Switch 1

# DAC SPI (DAC121S101) - PMOD connector
set_io dac_cs_n  65     # PMOD 1
set_io dac_sclk  64     # PMOD 2
set_io dac_mosi  63     # PMOD 3

# Status outputs - LEDs
set_io busy 56          # LED 1
set_io done 57          # LED 2
